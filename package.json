    "@blueprintjs/select": "^4.0.0",
    "@darkobits/vite-plugin-favicons": "^0.2.0",
    "@types/better-sqlite3": "^7.6.0",
    "@types/fuzzy-search": "^2.1.0",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^3.0.1",
    "better-sqlite3": "^7.6.2",
    "classnames": "^2.3.1",
    "cross-spawn": "^7.0.3",
    "eslint": "^7.32.0",
    "formdata-polyfill": "^4.0.10",
    "fuzzy-search": "^3.0.2",
    "he": "^1.2.0",
    "iconv-lite": "^0.6.3",
    "inquirer": "^8.2.0",
    "jimp": "^0.16.1",
    "jotai": "^1.4.6",
    "jsdom": "^17.0.0",
    "json-schema-to-typescript": "^10.1.5",
    "jsonschema": "^1.2.2",
    "node-fetch": "^2.6.7",
    "normalize.css": "^8.0.1",
    "p-queue": "^6.0.0",
    "prettier": "^2.4.0",
