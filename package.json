    "react-feather": "^2.0.9",
    "react-intl": "^5.23.0",
    "sanitize-filename": "^1.6.3",
    "surge": "^0.23.0",
    "typescript": "^4.5.2",
    "victory": "^36.6.8",
    "vite": "^4.0.4",
    "vite-plugin-pwa": "^0.14.1",
    "xml2js": "^0.4.23",
    "zustand": "^3.6.4"
